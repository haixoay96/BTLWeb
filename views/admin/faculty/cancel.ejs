<h3>Hủy đăng ký</h3>
<div class="form-group">
    <div class="col-sm-10">
        <form action="/update/cancel" method="post">
            Sinh viên:<br>
            <select name="MaSv" required="true" class="form-control">
                <% svs.forEach(function(sv){ %>
                    <option value=<%= sv.MaSv %>> <%=sv.HoTen%></option>
                 <% }); %>
            </select>
            <br>
            <input type="submit" value="Hủy" class="btn btn-info"">
        </form>
    </div>
</div>
