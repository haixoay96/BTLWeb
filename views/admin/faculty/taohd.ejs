<form action="/insert/hd" method="post">
    <div class="form-group">
        <label class="control-label col-sm-3">Tên hội đồng</label>
        <div class="col-sm-9">
            <input type="text" required="true" class="form-control" name="TenHd">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-3">Tên đề tài</label>
        <div class="col-sm-9">
            <select name="TenDt" required="true" class="form-control">
                <% dts.forEach(function(dt){ %>
                    <option value=<%= dt.TenDt %>> <%=dt.TenDt%></option>
                 <% }); %>
            </select>
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-3">Tên giảng viên</label>
        <div class="col-sm-9">
            <select name="MaGv" required="true" class="form-control">
                <% gvs.forEach(function(gv){ %>
                    <option value=<%= gv.MaGv %>> <%=gv.HoTen%></option>
                 <% }); %>
            </select>
        </div>
    </div>
    <div class="form-group">
        <div class="col-sm-offset-3 col-sm-4">
            <input type="submit" class="btn btn-info">
        </div>
    </div>
</form>
